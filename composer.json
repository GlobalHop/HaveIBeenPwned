{
    "name": "xsist10/haveibeenpwned",
    "description": "Client for the \"Have I Been Pwned\" API",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Shone",
            "email": "xsist10@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "psr/log": "^1.0",
        "guzzlehttp/psr7": "^1.4",
        "php-http/message": "^1.6",
        "php-http/discovery": "^1.4",
        "php-http/client-common": "^1.7"
    },
    "suggest": {
        "php-http/curl-client": "cURL adapter support",
        "php-http/guzzle6-adapter": "Guzzle adapter support"
    },
    "autoload": {
        "psr-4": { "xsist10\\HaveIBeenPwned\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "xsist10\\HaveIBeenPwned\\Tests\\": "tests/" }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.7 || ^6.5",
        "monolog/monolog": "^1.23"
    }
}
